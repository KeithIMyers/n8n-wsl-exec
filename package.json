{
  "name": "n8n-nodes-wsl-exec",
  "version": "0.1.2",
  "description": "A n8n node to execute commands in a WSL2 container from a Windows-based n8n instance.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write nodes",
    "lint": "eslint nodes --ext .js,.ts",
    "lint:fix": "eslint nodes --ext .js,.ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/WslExec/WslExec.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^24.3.1",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/eslint-plugin": "~5.45",
    "@typescript-eslint/parser": "~5.45",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-core": "*",
    "n8n-workflow": "*"
  },
  "author": "Keith I Myers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KeithIMyers/n8n-wsl-exec.git"
  },
  "bugs": {
    "url": "https://github.com/KeithIMyers/n8n-wsl-exec/issues"
  }
}
